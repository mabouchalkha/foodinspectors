.modal-header
    h3.modal-title
        span data-i18n="DeleteConfirmTitle"
.modal-body
    p
        span data-i18n="DeleteConfirmBody"
.modal-footer
    button.btn.btn-link ng-click="$dismiss()"
        span data-i18n="Cancel"
    button.btn.btn-danger ng-click="$close()"
        i.fa.fa-times &nbsp
        span data-i18n="Delete"
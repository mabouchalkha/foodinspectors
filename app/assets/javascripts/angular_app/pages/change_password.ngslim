.modal-header
    h3.modal-title
        span data-i18n="Change password"
.modal-body
    p 
        span data-i18n="resetPasswordConfirm"
.modal-footer
    button.btn.btn-link ng-click="$dismiss()"
        span data-i18n="Cancel"
    button.btn.btn-primary ng-click="$close()"
        i.fa.fa-key &nbsp
        span data-i18n="Change password"